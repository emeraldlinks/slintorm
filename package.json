{
  "name": "slintorm",
  "version": "0.0.17",
  "description": "Minimal fully typed orm for typescript",
  "license": "MIT",
  "author": "Joseph Christopher",
  "type": "module",
  "homepage": "https://github.com/emeraldlinks/slintorm",
  "bugs": {
    "url": "https://github.com/emeraldlinks/slintormssues"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emeraldlinks/slintorm.git"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "test": "node --loader ts-node/esm --experimental-specifier-resolution=node src/example.ts"
  },
  "dependencies": {
    "mongodb": "^6.20.0",
    "mysql2": "^3.15.3",
    "path": "^0.12.7",
    "pg": "^8.16.3",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "ts-morph": "^27.0.2"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@types/pg": "^8.15.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3"
  }
}
